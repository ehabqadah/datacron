spark.app.trajectories=cab_nds
spark.app.trajectories.statistics.input=cab_stats

spark.app.trajectories.with.jumps.speed.output=cab_jumps_speed
spark.app.trajectories.without.jumps.speed.output=cab_without_jumps_speed

spark.app.trajectories.with.jumps.dist.output=cab_jumps_dist
spark.app.trajectories.without.jumps.dist.output=cab_without_jumps_dist

spark.app.trajectories.with.duplicate.ids.output=cab_with_duplicate_id
spark.app.trajectories.without.duplicate.ids.output=cab_without_duplicate_id

spark.app.trajectories.jumps.speed.th.factor=1
spark.app.trajectories.quantile.speed.p=1

spark.app.trajectories.jumps.distance.th.factor=1
spark.app.trajectories.quantile.distance.p=1

spark.app.trajectories.dupid.th.factor=10

spark.app.trajectories.physicaltime=true
spark.app.trajectories.geocoordinates=true