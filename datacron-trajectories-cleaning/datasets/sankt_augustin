spark.app.trajectories=sankt_augustin_nds
spark.app.trajectories.statistics.input=sankt_augustin_stats

spark.app.trajectories.with.jumps.speed.output=sankt_augustin_jumps_speed
spark.app.trajectories.without.jumps.speed.output=sankt_augustin_without_jumps_speed

spark.app.trajectories.with.jumps.dist.output=sankt_augustin_jumps_dist
spark.app.trajectories.without.jumps.dist.output=sankt_augustin_without_jumps_dist

spark.app.trajectories.with.duplicate.ids.output=sankt_augustin_with_duplicate_id
spark.app.trajectories.without.duplicate.ids.output=sankt_augustin_without_duplicate_id

spark.app.trajectories.jumps.speed.th.factor=2
spark.app.trajectories.quantile.speed.p=0.29

spark.app.trajectories.jumps.distance.th.factor=3
spark.app.trajectories.quantile.distance.p=0.7

spark.app.trajectories.dupid.th.factor=20

spark.app.trajectories.physicaltime=true
spark.app.trajectories.geocoordinates=true